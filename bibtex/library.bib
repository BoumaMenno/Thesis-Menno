journal = {IEEE Transactions on Robotics},
keywords = {Biped robots,holonomic servoconstraints,limit-cycle walking,orbital stabilization,trajectory planning with dynamic constraints,transverse linearization,underactuated mechanical systems,virtual holonomic constraints},
number = {3},
pages = {589--601},
title = {{Stable walking gaits for a three-link planar biped robot with one actuator}},
volume = {29},
year = {2013}
}
