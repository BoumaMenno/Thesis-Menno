keywords = {Action Potentials,Action Potentials: physiology,Animals,Artificial Intelligence,Computer Communication Networks,Computer Simulation,Computer-Assisted,Humans,Models,Nerve Net,Nerve Net: physiology,Neurological,Signal Processing,Synaptic Transmission,Synaptic Transmission: physiology},
number = {5},
pages = {1314--25},
pmid = {18784014},
title = {{Synchronization and state estimation for discrete-time complex networks with distributed delays.}},
